{
    "name": "georgehadjisavva/elevenlabs-api-client",
    "description": "Wrapper client for eleven api text to voice api",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Georgehadjisavva\\ElevenApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Georgehadjisavva\\ElevenApiClient\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "role": "Author",
            "name": "George Hadjisavva",
            "email": "georgex8@gmail.com",
            "homepage": "https://georgehadjisavva.dev/"
        }
    ],
    "minimum-stability": "dev",

    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.7"
    },
    "require-dev":{
        "pestphp/pest": "^2.6",
        "vimeo/psalm": "^5.12"
    },
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": "dist"
    }
}
